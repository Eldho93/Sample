<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.eldho.sampleproject.view.fragment.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment_to_fragmentGenerate"
            app:destination="@id/fragmentGenerate" >
            <argument
                android:name="sellerName"
                app:argType="string" />
            <argument
                android:name="grossPrice"
                app:argType="string" />
            <argument
                android:name="grossWeight"
                app:argType="string" />
        </action>
    </fragment>
    <fragment
        android:id="@+id/fragmentGenerate"
        android:name="com.eldho.sampleproject.view.fragment.GenerateFragment"
        android:label="FragmentGenerate" >
        <action
            android:id="@+id/action_fragmentGenerate_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>

</navigation>